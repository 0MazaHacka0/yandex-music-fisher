{
    "manifest_version": 2,
    "name": "Yandex Music Fisher",
    "description": "Скачивание музыки с Yandex Music",
    "version": "0.01",
    "permissions": [
        "tabs",
        "downloads",
        "https://music.yandex.ru/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
