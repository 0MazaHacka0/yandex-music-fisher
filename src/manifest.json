{
    "manifest_version": 2,
    "name": "Yandex Music Fisher",
    "description": "Скачивание музыки с Yandex Music",
    "version": "0.0.2",
    "permissions": [
        "tabs",
        "downloads",
        "https://music.yandex.ru/"
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    },
    "page_action": {
        "default_icon": "img/icon.png"
    }
}
