{
    "manifest_version": 2,
    "name": "Yandex Music Fisher",
    "description": "Получение информации о музыки со страниц Yandex Music.",
    "version": "1.0",
    "permissions": [
        "tabs",
        "downloads",
        "https://music.yandex.ru/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
